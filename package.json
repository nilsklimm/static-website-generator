{
  "name": "static-website-generator",
  "version": "0.0.4",
  "main": "index.js",
  "license": "MIT",
  "author": "Nils Klimm <dev@nilsklimm.de>",
  "scripts": {
    "prepublishOnly": "npm run build",
    "postinstall": "npm run build",
    "cleanDistDir": "mkdir -p dist/ && rm -rf dist/*",
    "cleanStaticDir": "mkdir -p STATIC_WEBSITE/ && rm -rf STATIC_WEBSITE/*",
    "test:lint": "eslint src/**",
    "test": "npm run test:lint",
    "build": "npm run cleanDistDir && webpack --mode production -p --env.NODE_ENV=production",
    "develop": "webpack-dev-server --mode development --env.NODE_ENV=development",
    "start": "babel-node src/server/startServer.js",
    "renderToStatic": "npm run cleanStaticDir && babel-node src/server/renderToStatic.js"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "circular-dependency-plugin": "^5.0.2",
    "eslint": "^5.14.1",
    "eslint-plugin-react": "^7.12.4",
    "fetch-mock": "^7.3.0",
    "html-webpack-plugin": "^3.2.0",
    "react-hot-loader": "^4.7.1",
    "redux-devtools-extension": "^2.13.8",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "connected-react-router": "^6.3.1",
    "emotion": "^10.0.7",
    "emotion-server": "^10.0.7",
    "hapi": "^18.1.0",
    "history": "^4.7.2",
    "inert": "^5.1.2",
    "lodash.flatten": "^4.4.0",
    "lodash.map": "^4.6.0",
    "lodash.reduce": "^4.6.0",
    "lodash.startcase": "^4.4.0",
    "lowdb": "^1.0.0",
    "opn": "^5.4.0",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1",
    "uuid": "^3.3.2"
  }
}
